@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix math: <http://www.w3.org/2000/10/swap/math#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix string: <http://www.w3.org/2000/10/swap/string#> .
@prefix log: <http://www.w3.org/2000/10/swap/log#> .
@prefix list: <http://www.w3.org/2000/10/swap/list#> .
@prefix tpl: <http://niche.cs.dal.ca/ui_tpl.n3#> .
@prefix : <http://niche.cs.dal.ca/copd_ui.n3#> .
@base <http://niche.cs.dal.ca/copd_ui.n3> .


{	?tpl a tpl:UiTemplate ;
		tpl:select [ rdf:value ?selection ] ;
		tpl:generate [
			tpl:ui ?element ;
			tpl:placeholders ?placeholders ;
			tpl:values ?values ;
			tpl:target ?target
		] .
	
	( 	?selection
		{ 	?element tpl:output ?output .
			( ?placeholders "|" ) string:join ?regex .
			( ?output ?regex "%s" ) string:replace ?output2 .
			( ( ?output2 ) ?values ) list:append ?appended .
			?appended string:format ?output3 
		} 
	) log:conjunction ?premise .

} => { ?premise => { ?target tpl:generated ?output3 } } .


{	?el tpl:generated ?gen .

	?container ?property ?el .
	?property!log:uri string:notMatches tpl:!log:uri .

} => { ?container tpl:generated ?gen } .


{	?container ?property ?list .
	?list log:rawType log:List .
	?property!log:uri string:notMatches tpl:!log:uri .

	(?gen
	{	?list list:member ?el . 
		?el tpl:generated ?gen .
		 
	} ?gens) log:collectAllIn _:x . 
	
	?gens list:length ?len .
	?len math:greaterThan 0 .
	#?list!list:length math:equalTo ?len .

} => { ?container tpl:combined ?gens } .

